Jenkinsfile (Declarative Pipeline)
pipeline {
    agent any
Enviroment{
    stages {
        stage('Build') {
            steps {
                echo 'Building..'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
            enviroment('QA"){
            
            echo 'Deploying to QA'
        }
        }
    }
}}
